@article{luedtkeTranscranialDirectCurrent2012,
  title = {Transcranial {{Direct Current Stimulation}} for the {{Reduction}} of {{Clinical}} and {{Experimentally Induced Pain}}: {{A Systematic Review}} and {{Meta-analysis}}},
  shorttitle = {Transcranial {{Direct Current Stimulation}} for the {{Reduction}} of {{Clinical}} and {{Experimentally Induced Pain}}},
