  shorttitle = {Regulatory {{Considerations}} for the {{Clinical}} and {{Research Use}} of {{Transcranial Direct Current Stimulation}} ({{tDCS}})},
  author = {Fregni, F. and Nitsche, M. A. and Loo, C. K. and Brunoni, A. R. and Marangolo, P. and Leite, J. and Carvalho, S. and Bolognini, N. and Caumo, W. and Paik, N. J. and Simis, M. and Ueda, K. and Ekhitari, H. and Luu, P. and Tucker, D. M. and Tyler, W. J. and Brunelin, J. and Datta, A. and Juan, C. H. and Venkatasubramanian, G. and Boggio, P. S. and Bikson, M.},
