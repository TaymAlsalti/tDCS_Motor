@article{brunoniTranscranialDirectCurrent2016,
  title = {Transcranial Direct Current Stimulation for Acute Major Depressive Episodes: Meta-Analysis of Individual Patient Data},
  shorttitle = {Transcranial Direct Current Stimulation for Acute Major Depressive Episodes},
  author = {Brunoni, Andr{\'e} R. and Moffa, Adriano H. and Fregni, Felipe and Palm, Ulrich and Padberg, Frank and Blumberger, Daniel M. and Daskalakis, Zafiris J. and Bennabi, Djamila and Haffen, Emmanuel and Alonzo, Angelo and Loo, Colleen K.},
