  title = {Anodal {{tDCS Combined With Radial Nerve Stimulation Promotes Hand Motor Recovery}} in the {{Acute Phase After Ischemic Stroke}}},
  author = {Sattler, Virginie and Acket, Blandine and Raposo, Nicolas and Albucher, Jean-Fran{\c c}ois and Thalamas, Claire and Loubinoux, Isabelle and Chollet, Fran{\c c}ois and {Simonetta-Moreau}, Marion},
