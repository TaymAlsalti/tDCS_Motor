}

@article{staggCorticalActivationChanges2012,
  title = {Cortical Activation Changes Underlying Stimulation-Induced Behavioural Gains in Chronic Stroke},
  author = {Stagg, Charlotte Jane and Bachtiar, Velicia and O'Shea, Jacinta and Allman, Claire and Bosnell, Rosemary Ann and Kischka, Udo and Matthews, Paul McMahan and {Johansen-Berg}, Heidi},
  year = {2012},
