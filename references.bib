@article{jonesHighPrevalenceLow2005,
  title = {High Prevalence but Low Impact of Data Extraction and Reporting Errors Were Found in {{Cochrane}} Systematic Reviews},
  author = {Jones, Ashley P. and Remmington, Tracey and Williamson, Paula R. and Ashby, Deborah and Smyth, Rosalind L.},
